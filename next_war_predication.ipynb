      "id": "8c2311aa-abfa-41b0-aa93-3a9cfe7ee0c9",
      "metadata": {
        "id": "8c2311aa-abfa-41b0-aa93-3a9cfe7ee0c9"
      },
      "outputs": [],
      "source": [
        "batting[\"war_season\"] = batting.groupby(\"Season\", group_keys=False).apply(group_averages)"
      ]
