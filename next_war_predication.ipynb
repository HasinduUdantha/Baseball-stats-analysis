      "source": [
        "def next_season(player):    # This defines a function called next_season that takes a DataFrame (player) as input. This DataFrame will represent the data for a single player.\n",
        "    player = player.sort_values(\"Season\")     #Sorts the player's data by \"Season\" in ascending order to ensure the data is in chronological order.\n",
        "    player[\"Next_WAR\"] = player[\"WAR\"].shift(-1)    # creates a new column called \"Next_WAR\" and assigns to it the values from the \"WAR\" column shifted up by one position (shift(-1)).\n",
        "    return player\n",
        "\n",
        "batting = batting.groupby(\"IDfg\", group_keys=False).apply(next_season)"
      ]
